{
  "name": "endelwar/twig-extensions",
  "description": "Some helpful Twig extensions",
  "license": "MIT",
  "homepage": "https://github.com/endelwar/twig-extensions#twigextensions",
  "authors": [
    {
      "name": "Manuel Dalla Lana",
      "email": "endelwar@aregar.it"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "ext-mbstring": "*",
    "twig/twig": "^3.17",
    "gregwar/image": "^2.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "Endelwar\\Twig\\": "src/Endelwar/Twig"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Endelwar\\Twig\\Tests\\": "tests/Twig"
    }
  }
}
